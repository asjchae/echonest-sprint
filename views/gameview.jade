extends layout

block content
    .container-fluid
      .row-fluid
        span.span4
        span.span3
          div
            h1.dom-heading1 #{theme}
        span.span4
      .row-fluid
        each song, i in songs 
          span.span2
            .div-1
              h4.dom-heading #{song.name}
              h4.dom-heading
                | #{song.artist}
                div
                  br
              button.btn.btn-1.btn-2 Play
              button.btn.btn-2.btn-3 Submit
          